#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules/jiti/bin/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules/jiti/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules/jiti/bin/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules/jiti/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/jiti@1.18.2/node_modules:/Users/woueziou/Sources/Noloop/chez-wiyao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jiti@1.18.2/node_modules/jiti/bin/jiti.js" "$@"
else
  exec node  "$basedir/../../../../../jiti@1.18.2/node_modules/jiti/bin/jiti.js" "$@"
fi
